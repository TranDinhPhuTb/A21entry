<%= render "common/menu" %>
<div>
	<%= form_for(@user, url: custom_user_path) do |form| %>
	<%= form.label :email %>
	<%= form.email_field :email %><br>
	<%= form.label :first_name %>
	<%= form.text_field :first_name %><br>
	<%= form.label :last_name %>
	<%= form.text_field :last_name %><br>
	<%= form.submit %>
	<% end %>
</div>