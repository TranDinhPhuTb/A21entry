<%= render "common/menu" %>
<table>
<tr>
	<td>Email</td>
	<td>First name</td>
	<td>Last name</td>
	<td>Edit</td>
	<td>Delete</td>
</tr>
<% @users.each do |user| %>
<tr>
	<td><%= user.email %></td>
	<td><%= user.first_name || "-" %></td>
	<td><%= user.last_name || "-" %></td>
	<td><%= link_to "Edit", edit_custom_user_path(user.id) %></td>
	<td><%= link_to "Delete", custom_user_path(user.id), method: :delete, data: { confirm: "Are you sure you want to delete this users?" } %></td>
</tr>
<% end %>
</table>